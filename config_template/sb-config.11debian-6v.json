	  "output": "/etc/sing-box/sing-box.log",
      "timestamp": true
    },
    "experimental": {
      "clash_api": {
        "external_controller": "10.10.10.14:80",
        "external_ui": "/etc/sing-box/ui",
        "secret": "123654",
