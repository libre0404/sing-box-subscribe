  "dns": {
    "servers": [
      {
        "type": "dhcp",
		"tag": "localDns",
        "interface": "ens18"
      },
      {
        "type": "fakeip",
        "tag": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "rule_set": "geosite-cn",
		"action": "route",
        "server": "fakeip"
      },
      {
        "rule_set": "geosite-cn",
		"action": "route",
        "server": "localDns"
      },
      {
        "clash_mode": "direct",
		"action": "route",
        "server": "localDns"
      },
      {
        "clash_mode": "global",
		"action": "route",
        "server": "localDns"
      },
      {
        "rule_set": "geosite-geolocation-!cn",
		"action": "route",
        "server": "localDns"
      },
        {
          "type": "logical",
          "mode": "and",
          "rules": [
            {
              "rule_set": "geosite-geolocation-!cn",
              "invert": true
            },
            {
              "rule_set": "geoip-cn"
            }
          ],
		  "action": "route",
          "server": "localDns",
          "client_subnet": "114.114.114.114/24"
        },
        {
          "query_type": [
            "A",
            "AAAA"
          ],
		  "action": "route",
          "server": "fakeip"
        }
    ],
    "final": "localDns",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "tag": "tun-in",
      "type": "tun",
      "address": [
        "172.19.0.0/30",
		"fdfe:dcba:9876::1/126"
      ],
      "mtu": 9000,
      "auto_route": true,
      "strict_route": true,
      "stack": "system",
      "platform": {
        "http_proxy": {
          "enabled": true,
          "server": "127.0.0.1",
          "server_port": 2080
        }
      }
    },
    {
      "tag": "mixed-in",
      "type": "mixed",
	  "domain_strategy": "prefer_ipv4",
      "listen": "127.0.0.1",
      "listen_port": 2080
